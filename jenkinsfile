pipeline {
    agent any

    stages {
        stage('CLONE') {
            steps {
                https://github.com/nakkasrihan/vansro22.git
		}
        }
	stage('BUILD') {
            steps {
                sh "mvn clean package"
            }
        }
	stage('DEPLOY") {
        	 steps {
		       'sshagent (['Tomcat-login']) {
   		        sh "scp -o StrictHostKeyChecking=no target/vansro-1.0-SNAPSHOT.jar@13.234.19.10:/opt/tomcat9/webapps" 
   		        
		    }

               }
        }
}
